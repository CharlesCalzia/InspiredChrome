{
    "name": "Youtrack",
    "version": "1.0",
    "author": "Charles Calzia",
    "description": "Extension to help you reduce the amount of time spent on Youtube!",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icon_128.png"
    },
    "permissions": [
        "*://*.youtube.com/*"
    ]
}